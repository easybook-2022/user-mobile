{
  "expo": {
    "name": "easygo-user",
    "slug": "easygo-user",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.geottuse.serviceapp.users",
      "buildNumber": "0",
      "infoPlist": {
        "NSCameraUsageDescription": "EasyGO uses camera to allow you to take a profile picture",
        "NSLocationWhenInUseUsageDescription": "EasyGO uses your location to show you the nearest locations",
        "NSLocationUsageDescription": "EasyGO uses your location to show you the nearest locations"
      }
    },
    "android": {
      "package": "com.geottuse.serviceapp.users"
    },
    "notification": {
      "icon": "./assets/notification.png"
    }
  }
}
